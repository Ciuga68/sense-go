{
  "name": "sense-go",
  "version": "0.12.9",
  "description": "Validate, package and deploy your Qlik Sense Visualization Extension projects with ease.",
  "keywords": [
    "deployment",
    "extensions",
    "grunt",
    "gulp",
    "less",
    "mashups",
    "packaging",
    "qlik",
    "qlik-sense",
    "qlik-sense-desktop",
    "qrs",
    "ssh",
    "validation",
    "visualization-extensions",
    "wbfolder.wbl",
    "web-projects",
    "zip"
  ],
  "homepage": "https://github.com/stefanwalther/sense-go",
  "bugs": {
    "url": "https://github.com/stefanwalther/sense-go/issues"
  },
  "license": "MIT",
  "author": "Stefan Walther (https://github.com/stefanwalther)",
  "main": "./lib/index.js",
  "bin": {
    "sense-go": "bin/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/stefanwalther/sense-go.git"
  },
  "scripts": {
    "test": "mocha --require ./test/mocha.conf.js test/*.spec.js",
    "docs": "docker run --rm -v ${PWD}:/opt/verb stefanwalther/verb",
    "test-coverage": "istanbul cover node_modules/.bin/_mocha  --require ./test/mocha.conf.js -- -- -u exports -R spec test/**/* && codecov",
    "lint": "eslint lib && eslint test",
    "lint:src": "eslint lib",
    "lint:src:fix": "eslint lib --fix",
    "lint:test": "eslint test",
    "lint:test:fix": "eslint test --fix"
  },
  "dependencies": {
    "archy": "^1.0.0",
    "async": "^2.4.1",
    "chalk": "^1.1.3",
    "deep-extend": "^0.5.0",
    "del": "^3.0.0",
    "expand-tilde": "^2.0.2",
    "extend-shallow": "^2.0.1",
    "fs-extra": "^3.0.1",
    "glob": "^7.1.2",
    "graceful-fs": "^4.1.5",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "3.1.0",
    "gulp-babel": "^6.1.2",
    "gulp-bump": "^2.2.0",
    "gulp-clean-css": "^3.4.2",
    "gulp-concat": "^2.6.0",
    "gulp-debug": "^3.1.0",
    "gulp-filter": "^5.0.0",
    "gulp-git": "^2.4.0",
    "gulp-header": "^1.8.8",
    "gulp-htmlmin": "^3.0.0",
    "gulp-if": "^2.0.1",
    "gulp-jsonlint": "^1.1.2",
    "gulp-jsonminify": "^1.0.0",
    "gulp-less": "^3.1.0",
    "gulp-load-plugins": "^1.2.4",
    "gulp-nop": "0.0.3",
    "gulp-plumber": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace-task": "^0.11.0",
    "gulp-simple-task-loader": "^1.3.8",
    "gulp-sourcemaps": "^2.6.0",
    "gulp-ssh": "^0.6.0",
    "gulp-strip-debug": "^1.1.0",
    "gulp-tag-version": "^1.3.0",
    "gulp-touch": "^1.0.1",
    "gulp-uglify": "2.0.0",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^4.3.9",
    "gulp-zip": "^4.0.0",
    "js-yaml": "^3.8.4",
    "liftoff": "^2.3.0",
    "lodash": "^4.14.2",
    "minimatch": "^3.0.4",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "sanitize-filename": "^1.6.0",
    "sense-loc": "^0.4.0",
    "spawn-cmd": "0.0.2",
    "tildify": "^1.2.0",
    "time-stamp": "^2.0.0",
    "v8flags": "^2.0.11"
  },
  "devDependencies": {
    "assertion-error": "^1.0.2",
    "chai": "latest",
    "chai-files": "^1.4.0",
    "codecov": "^2.2.0",
    "eslint": "^4.0.0",
    "eslint-config-space-single": "^0.2.0",
    "istanbul": "^0.4.5",
    "mocha": "*",
    "mocha-lcov-reporter": "^1.3.0",
    "should": "*"
  },
  "engines": {
    "node": ">=7.2"
  },
  "preferGlobal": true,
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 2,
      "footer": " "
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "data": {
      "twitter": "waltherstefan",
      "travis_url": "stefanwalther"
    }
  }
}
